{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noEmit": false,
    "baseUrl": "."
  }
}
